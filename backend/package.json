{
  "name": "backend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "prestart": "docker-compose up -d",
    "start": "nodemon --trace-warnings src/server.js",
    "lint": "eslint . --ext .js --ignore-pattern 'node_modules'",
    "format": "prettier --write src/*.js",
